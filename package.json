{
  "name": "jukebox",
  "version": "1.0.0",
  "description": "Remote audio player (on Raspberry Pi)?",
  "author": "unblee",
  "license": "MIT",
  "homepage": "https://github.com/unblee/jukebox",
  "repository": {
    "type": "git",
    "url": "https://github.com/unblee/jukebox.git"
  },
  "main": "src/app.js",
  "scripts": {
    "start": "node ./src/app.js",
    "dev": "nodemon --watch src ./src/app.js",
    "precommit": "lint-staged",
    "lint": "eslint --ext .js",
    "lint:all": "eslint . --ext .js",
    "test": "mocha",
    "test:all": "mocha --recursive features/ --recursive units/",
    "coverage": "nyc mocha --recursive features/ --recursive units/",
    "e2e": "nightwatch --config nightwatch.conf.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "eslint . --ext .js --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "nyc": {
    "reporter": [
      "text",
      "text-summary",
      "lcov",
      "html"
    ],
    "exclude": [
      "**/node_modules/**",
      "tests/",
      "coverage/"
    ],
    "all": true
  },
  "dependencies": {
    "await-lock": "^1.1.2",
    "debug": "^3.0.0",
    "dotenv": "^4.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-favicon": "^2.0.0",
    "koa-mount": "^3.0.0",
    "koa-route": "^3.2.0",
    "koa-router": "^7.2.1",
    "koa-static": "^4.0.1",
    "koa-websocket": "^4.0.0",
    "lodash.debounce": "^4.0.8",
    "lodash.random": "^3.2.0",
    "lodash.shuffle": "^4.2.0",
    "lodash.throttle": "^4.1.1",
    "pcm-volume": "^1.0.0",
    "promise-memorize": "^1.1.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "server-destroy": "^1.0.1",
    "speaker": "^0.3.0",
    "timed-stream": "^1.1.0",
    "ytdl-core": "^0.15.0"
  },
  "devDependencies": {
    "chromedriver": "^2.43.3",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.1.2",
    "geckodriver": "^1.8.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.2",
    "mocha": "^3.5.0",
    "nightwatch": "^0.9.16",
    "nodemon": "^1.11.0",
    "nyc": "^11.1.0",
    "power-assert": "^1.4.4",
    "prettier": "^1.5.3",
    "prettier-eslint-cli": "^4.1.1",
    "selenium-server": "^3.4.0",
    "sinon": "^3.0.0",
    "supertest": "^3.0.0"
  }
}
